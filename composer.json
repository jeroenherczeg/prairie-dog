{
    "name": "jeroenherczeg/prairie-dog",
    "type": "library",
    "description": "A Stream Generator for Laravel 5",
    "keywords": [
        "jeroenherczeg",
        "prairie-dog"
    ],
    "homepage": "https://github.com/jeroenherczeg/prairie-dog",
    "license": "MIT",
    "authors": [
        {
            "name": "Jeroen Herczeg",
            "email": "jeroen@herczeg.be",
            "homepage": "https://herczeg.be",
            "role": "Developer"
        }
    ],
    "require": {
        "php" : "~5.6|~7.0"
    },
    "require-dev": {
        "phpunit/phpunit" : "~4.0||~5.0",
        "scrutinizer/ocular": "~1.1",
        "squizlabs/php_codesniffer": "~2.3"
    },
    "autoload": {
        "psr-4": {
            "jeroenherczeg\\prairie-dog\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "jeroenherczeg\\prairie-dog\\": "tests"
        }
    },
    "scripts": {
        "test": "phpunit",
        "format": "phpcbf --standard=psr2 src/"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    },
    "config": {
        "sort-packages": true
    }
}
